if (document.getElementById("specialLink") == null) {
    var a  = document.createElement('link');
    var b  = document.createElement('link');

    a.rel  = "stylesheet";
    a.href = "https://realmadsci.github.io/j/glitch.css";
    a.id   = "specialLink";
    b.rel  = "stylesheet";
    b.href = "https://fonts.googleapis.com/css2?family=Syne+Mono&display=swap";

    document.head.appendChild(a);
    document.head.appendChild(b);
}

document.addEventListener('DOMContentLoaded', function() {
    var elements = document.getElementsByClassName("specialName");
    if (elements.length != 1) {
            var div = document.createElement('div');
            div.class = 'specialMessage';
            div.innerHTML = 'smiddy was here';
 
            document.body.appendChild(div);
        }
    }, false);

